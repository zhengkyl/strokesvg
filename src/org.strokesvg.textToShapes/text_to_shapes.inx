<?xml version="1.0" encoding="UTF-8"?>
<inkscape-extension xmlns="http://www.inkscape.org/namespace/inkscape/extension">
  <name>Text to Shapes</name>
  <id>org.strokesvg.textToShapes</id>
  <param name="text" type="string" gui-text="Text:"></param>
  <effect>
    <object-type>all</object-type>
    <effects-menu>
      <submenu name="Text"/>
    </effects-menu>
  </effect>
  <script>
    <command location="inx" interpreter="python">text_to_shapes.py</command>
  </script>
</inkscape-extension>
